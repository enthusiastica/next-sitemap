{
  "build": {
    "commands": [
      "yarn build"
    ]
  },
  "clean": {
    "commands": [
      "rm -rf dist",
      "rm -rf build"
    ]
  },
  "set-version": {
    "commands": [
      "npm version --no-git-tag-version ${BUILD_BUILDNUMBER}"
    ]
  },
  "test": {
    "commands": [
      "yarn test"
    ]
  }
}